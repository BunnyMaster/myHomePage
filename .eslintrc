{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", ["prettier"], ["plugin:prettier/recommended"]],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  // 要加上这个
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}
